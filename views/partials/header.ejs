    <!-- Bootstrap 4.3.1 -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.1/css/all.css">

    <!-- Including custom stylesheets -->
    <link rel="stylesheet" href="/stylesheets/main.css">

    <title>YelpCamp</title>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-151222018-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-151222018-1');
    </script>

</head>

<body class="d-flex flex-column h-100">
    <header>
        <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
            <a class="navbar-brand" href="/"><img src="../../public/img/favicon.jpg" style="height:30px; width: 30px;"> YelpCamp</a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggle"
                aria-controls="navbarToggle" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarToggle">
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                    <li class="nav-item <%= typeof page !== 'undefined' && page === 'campgrounds' ? 'active' : '' %>">
                        <a class="nav-link" href="/campgrounds">Campgrounds</a>
                    </li>
                    <li class="nav-item <%= typeof page !== 'undefined' && page === 'users' ? 'active' : '' %>">
                        <a class="nav-link" href="/users">Members</a>
                    </li>
                </ul>
                <% if (!currentUser) { %>
                <a class="btn btn-outline-light btn-space" href="/campgrounds/new">
                    <i class="fas fa-plus"></i> New
                </a>
                <a class="btn btn-info btn-space" href="/login">
                    <i class="far fa-paper-plane">&nbsp;</i>Login
                </a>
                <a class="btn btn-outline-info btn-space" href="/register">
                    <i class="fas fa-chevron-circle-up">&nbsp;</i>Register
                </a>
                <% } else { %>
                <a class="btn btn-outline-light btn-space" href="/campgrounds/new">
                    <i class="fas fa-plus"></i> New
                </a>
                <a class="btn btn-outline-light btn-space" href="/users/<%=currentUser.id %>">
                    <i class="fas fa-user"></i> Profile
                </a>
                <a href="/logout" class="btn btn-outline-light btn-space">
                    <i class="fas fa-sign-out-alt"></i> Logout
                </a>
                <% } %>
            </div>
        </nav>
    </header>
